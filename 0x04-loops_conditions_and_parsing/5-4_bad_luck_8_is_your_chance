#!/usr/bin/env bash
# Purpose: Loops from 1 to 10 and displays different messages based on the iteration.

# Counter variable
count=0

# Loop until count is less than 10
while [ $count -lt 10 ]; do
  # Check iteration count and display the corresponding message
  if [ $count -eq 3 ]; then
    echo "bad luck"
  elif [ $count -eq 7 ]; then
    echo "good luck"
  else
    echo "Best School"
  fi
  ((count++))
done
